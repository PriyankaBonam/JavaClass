/*
0
0       1
0       2       4
0       3       6       9
0       4       8       12      16
0       5       10      15      20      25
0       6       12      18      24      30      36
 */

import java.util.Scanner;
public class Pyramid5 {
public static void main(String args[])
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter rows : ");
	int rows=s.nextInt();
	for (int i=0;i<rows;i++)
	{
		for(int j=0;j<=i;j++)
			System.out.print(i*j);
		}
	System.out.println("");
}
}
