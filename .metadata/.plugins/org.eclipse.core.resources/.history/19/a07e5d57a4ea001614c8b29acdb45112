
class DataTypes
{
 public static void main(String args[])
  { 
   byte a; 
    a=127; 
    System.out.println(a);
    a=-128;
    System.out.println(a);
    /* the range of byte is -128 to 127 so when a is assigned with value not in range
     (eg : -129 or 128 Type Mismatch Error is displayed*/
    a=0.0;
    System.out.println(a);
    }
    
    }
    