/*
         *
       * *
     * * *
   * * * *
 * * * * *
 
 */
import java.util.Scanner;
public class Pyramid6 {
	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		System.out.println("Enter rows : ");
		int rows = s.nextInt();
		for(int i=0;i<rows;i++)
		{
			for(int j=1;j<=rows;j++)
			{
				System.out.print("i");
			}
		}
	}

}
